{
  "name": "components",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --no-cache --continue",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "eslint-config-custom": "*",
    "postcss": "^8.4.21",
    "prettier": "2.8.7",
    "tailwindcss": "^3.3.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@7.24.0",
  "dependencies": {
    "clsx": "^1.2.1",
    "react-router-dom": "^6.10.0",
    "vite-plugin-svgr": "^2.4.0"
  }
}
